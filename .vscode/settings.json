        "/usr/include/harfbuzz",
        "/usr/include/sigc++-2.0",
        "/usr/lib/sigc++-2.0/include"
        */
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.hg/store/**": true,
        "**/deps": true,
        "**/build-*": true,
    },
    "C_Cpp.default.defines": [
        "COCOA_PROJECT=1",
        "SK_VULKAN"
