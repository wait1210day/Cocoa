{
    // "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.includePath": [
        "include",
        "deps/skia",
        "deps/v8",
        "deps/v8/include",
        "/usr/include/libdrm"
        /*
        "/usr/include/cairomm-1.0",
